{
  "name": "vue-example",
  "version": "1.0.0",
  "description": "Integration test project for eslint-plugin-valid-class-name Vue support",
  "type": "module",
  "scripts": {
    "lint": "eslint src --ext .vue",
    "lint:fix": "eslint src --ext .vue --fix",
    "test:valid": "eslint src/components/ValidComponent.vue",
    "test:invalid": "eslint src/components/InvalidComponent.vue",
    "test:static": "eslint src/components/StaticClasses.vue",
    "test:dynamic": "eslint src/components/DynamicClasses.vue",
    "test:object-syntax": "eslint src/components/ObjectSyntax.vue",
    "test:array-syntax": "eslint src/components/ArraySyntax.vue",
    "test:ternary": "eslint src/components/TernaryExpressions.vue",
    "test:mixed": "eslint src/components/MixedClasses.vue",
    "test:tailwind-utilities": "eslint src/components/TailwindUtilities.vue",
    "test:tailwind-variants": "eslint src/components/TailwindVariants.vue",
    "test:arbitrary-values": "eslint src/components/ArbitraryValues.vue",
    "test:edge-cases": "eslint src/components/EdgeCases.vue",
    "test:all-valid": "eslint src/components/ValidComponent.vue src/components/StaticClasses.vue src/components/DynamicClasses.vue src/components/ObjectSyntax.vue src/components/ArraySyntax.vue src/components/TernaryExpressions.vue src/components/MixedClasses.vue src/components/TailwindUtilities.vue src/components/TailwindVariants.vue src/components/ArbitraryValues.vue src/components/EdgeCases.vue",
    "test:all-invalid": "eslint src/components/InvalidComponent.vue"
  },
  "dependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "eslint": "^9.38.0",
    "eslint-plugin-valid-class-name": "file:../..",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3",
    "vue-eslint-parser": "^10.2.0"
  }
}
